{
    "id":"process1",
    "name":"noomi flow test",
    "children":[
        {
            "node":"StartEvent",
            "id":"start1"
        },{
            "node":"SequenceFlow",
            "id":"sequence1",
            "sourceRef":"start1",
            "targetRef":"userTask1"
        },{
            "node":"UserTask",
            "id":"userTask1",
            "implementation":"UserService.request",
            "potentialOwner":{
                "resourceRef":"user",
                "type":"group",
                "resourceAssignmentExpression":"user"
            },
            "rendering":{
                "id":"reqForm",
                "url":"/user/req"
            }
        },{
            "node":"SequenceFlow",
            "id":"sequence2",
            "sourceRef":"userTask1",
            "targetRef":"userTask2"
        },{
            "node":"UserTask",
            "id":"userTask2",
            "implementation":"UserService.check",
            "potentialOwner":{
                "resourceRef":"user",
                "type":"group",
                "resourceAssignmentExpression":"admin*"
            },
            "rendering":{
                "id":"checkForm",
                "url":"/user/check"
            }
        },{
            "node":"SequenceFlow",
            "id":"sequence3",
            "sourceRef":"userTask2",
            "targetRef":"theEnd",
            "conditionExpression":"${verificationResult == 'OK'}"
        },{
            "node":"SequenceFlow",
            "id":"sequence4",
            "sourceRef":"userTask2",
            "targetRef":"scriptTask1",
            "conditionExpression":"${verificationResult == 'Not OK'}"
        },{
            "node":"ScriptTask",
            "id":"scriptTask1",
            "script":"console.log(${requestMsg})"
        },{
            "node":"SequenceFlow",
            "id":"sequence5",
            "sourceRef":"scriptTask1",
            "targetRef":"theEnd"
        },{
            "node":"EndEvent",
            "id":"theEnd"
        }
    ]
}